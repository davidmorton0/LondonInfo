<!DOCTYPE html>
<html>
  <%= render 'layouts/head' %>
  <body>
    <%= render 'layouts/header' %>
    <main>
      <div id="main-container">
        <div class="col-sm-2">
          <%= render 'layouts/sidebar' %>
        </div>
        <div class="col-sm-10">
          <%= yield %>
          <%= render 'layouts/footer' %>
          <%= debug(params) if Rails.env.development? %>
        </div>
      </main>
    </div>
  </body>
</html>